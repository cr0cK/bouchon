{
  "name": "bouchon",
  "version": "2.0.0-alpha.1",
  "description": "Efficient API mocking with cool libraries",
  "main": "dist/main.js",
  "bin": {
    "bouchon": "dist/server/index.js"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && babel --ignore '*-test.js' -d dist src",
    "build:flow": "flow-copy-source -v -i '**/__tests__/**' src dist",
    "start": "node dist/server/index.js",
    "start:dev": "NODE_PATH=$NODE_PATH:./src nodemon -w src -w tests index.js",
    "test": "npm run build && NODE_PATH=$NODE_PATH:./dist jest",
    "prepublish": "npm run build",
    "lint": "eslint .",
    "flow": "flow"
  },
  "author": "Alexis Mineaud <alexis.mineaud@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=4.0.0"
  },
  "dependencies": {
    "body-parser": "^1.14.1",
    "bunyan": "^1.5.1",
    "bunyan-format": "^0.2.1",
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "express": "^4.13.3",
    "lodash": "^4.11.1",
    "morgan": "^1.6.1",
    "redux": "^3.0.4",
    "redux-act": "^1.5.1",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.2.1",
    "babel-eslint": "^8.0.3",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "chai": "^4.1.2",
    "eslint": "^4.12.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.4.2",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "flow-bin": "^0.60.1",
    "flow-typed": "^2.2.3",
    "freeport": "^1.0.5",
    "jest": "^21.2.1",
    "nodemon": "^1.8.1",
    "request": "^2.67.0"
  },
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cr0cK/bouchon.git"
  },
  "keywords": [
    "bouchon",
    "mock",
    "api",
    "restful",
    "redux",
    "reselect",
    "reducer",
    "selector",
    "middleware"
  ],
  "bugs": {
    "url": "https://github.com/cr0cK/bouchon/issues"
  },
  "homepage": "https://github.com/cr0cK/bouchon#readme",
  "jest": {
    "verbose": false,
    "forceExit": true,
    "testEnvironment": "node",
    "testRegex": "/tests/.*-test.js$"
  }
}
